class Solution {
public:
    int majorityElement(vector<int>& nums) {
        map<int,int> mp;
        int max1 =0;
        for(int i=0;i<nums.size();i++)
        {
            mp[nums[i]]++;
            if(mp[nums[i]] > nums.size()/2)
            {
                max1 = nums[i];
                break;
            }
        }
        return max1;      
    }
};
